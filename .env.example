# Environment Variables Template
# Copy this file to .env and update with your local values

# Application Settings
VITE_APP_NAME=Funeral Services Website Design
VITE_APP_ENV=development

# Contact Information
VITE_CONTACT_PHONE_1=+91 98765 43210
VITE_CONTACT_PHONE_2=+91 98765 43211
VITE_CONTACT_EMAIL=care@thelaststring.com
VITE_CONTACT_LOCATION=Mumbai, Maharashtra, India

# API Configuration (if needed)
VITE_API_URL=http://localhost:8000/api
VITE_API_TIMEOUT=10000

# Feature Flags (if needed)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true



# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id
